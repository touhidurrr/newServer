{
  "name": "uncivserver.xyz",
  "version": "4.0.0",
  "description": "An Open Source, Unciv Multiplayer Server written in Javascript",
  "author": "Md. Touhidur Rahman",
  "license": "BSD-3-Clause",
  "repository": "touhidurrr/UncivServer.xyz",
  "homepage": "https://github.com/touhidurrr/UncivServer.xyz#readme",
  "bugs": {
    "url": "https://github.com/touhidurrr/UncivServer.xyz/issues"
  },
  "keywords": [
    "Unciv",
    "Multiplayer",
    "Server",
    "Discord",
    "Turn",
    "Notifications",
    "fastify",
    "Redis"
  ],
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --check '**/?*.*'",
    "fix:prettier": "prettier --write '**/?*.*'",
    "wc": "wc src/*.ts src/*/*.ts src/*/*/*.ts"
  },
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@elysiajs/bearer": "^1.0.2",
    "@elysiajs/static": "^1.0.2",
    "bytes": "^3.1.2",
    "discord-api-types": "^0.37.79",
    "elysia": "^1.0.13",
    "ioredis": "^5.3.2",
    "mongodb": "^6.5.0"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.4",
    "bun-types": "latest",
    "typescript": "^5.4.5"
  },
  "bun-create": {
    "start": "bun run src/index.ts"
  }
}
